  },
  "peerDependencies": {
    "svgo": "^3 || ^4"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.2.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@tomer/eslint-config": "^4.1.1",
    "@tomer/prettier-config": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
